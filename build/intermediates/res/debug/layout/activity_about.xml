                        android:contentDescription="@null" />

                    <TextView
                        style="@style/custom_textview2"
                        android:id="@+id/textview_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:text="@string/beike_email" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/activity_item_height" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:background="@drawable/ic_brower"
                        android:contentDescription="@null" />

                    <TextView
                        style="@style/custom_textview2"
                        android:id="@+id/textview_website"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:text="@string/beike_website" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

