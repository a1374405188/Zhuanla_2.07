<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/global_bg1"
    android:orientation="vertical"
    >
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/activity_item1_space"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dingqi_zhichan1"
        android:background="@color/global_text4"
        >
       <TextView
           android:id="@+id/tv1"
           android:layout_width="wrap_content"
           android:layout_height="match_parent"
           android:layout_alignParentLeft="true"
           android:text="30天定期"
           android:textColor="@color/global_text1"
           android:gravity="center"
           android:layout_marginLeft="8dp"
           android:textSize="@dimen/dingqi_zhichan30"
           />
        <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:text="0.00元"
            android:textColor="@color/global_text1"
            android:gravity="center"
            android:layout_marginRight="8dp"
            android:textSize="@dimen/dingqi_zhichan30"
            />
     </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#ffd6d6d6"
        android:paddingLeft="1dp"
        android:paddingRight="1dp"
        android:paddingBottom="1dp"
        >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dingqi_zhichan2"
        android:background="#fffadee1"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:text="@string/dingqi_zhichan2"
            android:textColor="@color/global_text2"
            android:gravity="center"
            android:layout_marginLeft="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
        <TextView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:text="0.00元"
            android:textColor="@color/global_text3"
            android:gravity="center"
            android:layout_marginRight="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
    </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dingqi_zhichan2"
            android:background="#fff4f4f4"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:text="收益率"
                android:textColor="@color/global_text2"
                android:gravity="center"
                android:layout_marginLeft="8dp"
                android:textSize="@dimen/dingqi_zhichan31"
                />
            <TextView
                android:id="@+id/tv66"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:text="0%"
                android:textColor="@color/global_text3"
                android:gravity="center"
                android:layout_marginRight="8dp"
                android:textSize="@dimen/dingqi_zhichan31"
                />
        </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dingqi_zhichan2"
        android:background="#ffffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:text="@string/dingqi_zhichan3"
            android:textColor="@color/global_text2"
            android:gravity="center"
            android:layout_marginLeft="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
        <TextView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:text="0天"
            android:textColor="@color/global_text3"
            android:gravity="center"
            android:layout_marginRight="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dingqi_zhichan2"
        android:background="#fff4f4f4"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:text="@string/dingqi_zhichan4"
            android:textColor="@color/global_text2"
            android:gravity="center"
            android:layout_marginLeft="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
        <TextView
            android:id="@+id/tv5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:text="2015-01-18 11:05:36"
            android:textColor="@color/global_text3"
            android:gravity="center"
            android:layout_marginRight="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dingqi_zhichan2"
        android:background="#ffffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:text="@string/dingqi_zhichan5"
            android:textColor="@color/global_text2"
            android:gravity="center"
            android:layout_marginLeft="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
        <TextView
            android:id="@+id/tv6"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:text="2015-01-18 11:05:36"
            android:textColor="@color/global_text3"
            android:gravity="center"
            android:layout_marginRight="8dp"
            android:textSize="@dimen/dingqi_zhichan31"
            />
    </RelativeLayout>

    </LinearLayout>

    <View
        android:id="@+id/view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="16dp"
        ></View>
</LinearLayout>